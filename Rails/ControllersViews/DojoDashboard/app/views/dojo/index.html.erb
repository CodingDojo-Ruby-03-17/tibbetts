<h1>Dojo#index</h1>
<p>Find me in app/views/dojo/index.html.erb</p>
<table>
  <thead>
    <tr>
      <th>Coding Dojo Branch</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @dojos.each do |dojo| %>
      <tr>
        <td><%= dojo.branch %></td>
        <td><%= dojo.street %></td>
        <td><%= dojo.city %></td>
        <td><%= dojo.state %></td>
	<td><a href="/dojos/show/<%= dojo.id %>">Show</a> | <a href="/dojos/edit/<%= dojo.id %>">Edit</a> | <a href="/dojos/<%= dojo.id %>" data-method="delete">Destroy</a></td>
      </tr>
    <% end %>
  </tbody>
</table>

<a href="/dojos/new">New Dojo</a>
