<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_for @user, :url => { action: "create" } do |u| %>
  <%= u.label :first_name %>
  <%= u.text_field :first_name %>
  <%= u.label :last_name %>
  <%= u.text_field :last_name %>
  <%= u.label :username %>
  <%= u.text_field :username %>
  <%= u.label :email %>
  <%= u.text_field :email %>
  <%= u.label :password %>
  <%= u.text_field :password %>
  <%= submit_tag "Register User" %>
<% end %>

