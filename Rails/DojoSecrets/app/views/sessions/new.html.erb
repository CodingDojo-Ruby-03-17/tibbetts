<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_for @user, url: { action: "create" } do |user| %>
  <%= user.label :name %>
  <%= user.text_field :name %>
  <%= user.label :email %>
  <%= user.text_field :email %>
  <%= user.label :password %>
  <%= user.text_field :password %>
  <%= submit_tag "Log In" %>
<% end %>
